    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios14": "react-native run-ios --simulator iphone 14 pro",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "clean": "cd android && ./gradlew clean && cd ..",
    "apk": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "adb": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd ..",
    "pod": "cd ios && pod install && cd ..",
    "android:staging": "react-native run-android --variant=stagingdebug",
